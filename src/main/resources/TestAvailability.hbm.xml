<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="model.TestAvailability" table="Test_Availability">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="startDate" column="start_date" type="java.util.Date"/>
        <property name="endDate" column="end_date" type="java.util.Date"/>


        <many-to-one name="testId" class="model.Subjects" lazy="false">
            <column name="test_id" not-null="true"/>
        </many-to-one>


    </class>
</hibernate-mapping>