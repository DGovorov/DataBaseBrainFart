<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="model.TestSession" table="Test_Session">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="submitDate" column="submit_date" type="java.util.Date"/>
        <property name="testResult" column="test_result" type="java.lang.Integer"/>


        <many-to-one name="userId" class="model.Users" lazy="false">
            <column name="user_id" not-null="true"/>
        </many-to-one>


        <set name="questionResultses" table="Question_Results" lazy="false">
            <key>
                <column name="test_session_id"/>
            </key>
            <one-to-many class="model.QuestionResults"/>
        </set>


    </class>
</hibernate-mapping>