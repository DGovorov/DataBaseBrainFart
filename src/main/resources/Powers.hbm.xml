<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="modelAdmin.Powers" table="Powers">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="position" column="position" type="java.lang.String"/>


        <set name="admin" table="Admins" lazy="false">
            <key>
                <column name="powers_id"></column>
            </key>
            <one-to-many class="modelAdmin.Admins"/>
        </set>


    </class>
</hibernate-mapping>