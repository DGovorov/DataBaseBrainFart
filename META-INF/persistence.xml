<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="persistenceUnit">
        <class>models.AnswerOptions</class>
        <class>models.QuestionConnections</class>
        <class>models.QuestionResults</class>
        <class>models.Questions</class>
        <class>models.Subjects</class>
        <class>models.TestAvailability</class>
        <class>models.Tests</class>
        <class>models.TestSession</class>
        <class>models.Users</class>
        <properties>
            <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/BrainFartDB"/>
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
            <property name="hibernate.connection.username"/>
            <property name="hibernate.connection.password"/>
        </properties>
    </persistence-unit>
</persistence>
